{
  "name": "api3-dao-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build": "node dev-scripts/copy-localhost-data.js && react-scripts build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "eth:deploy-dao-contracts-on-hardhat": "node dev-scripts/deploy-dao-contracts-on-hardhat.js",
    "eth:node": "cd hardhat && npx hardhat node --network hardhat",
    "eth:prepare-dao-contracts-for-hardhat": "node dev-scripts/prepare-dao-contracts-for-hardhat.js",
    "eth:run-script": "cd hardhat && npx hardhat run",
    "fix-dev-issues": "node dev-scripts/prepare-dao-contracts-for-typechain.js && node dev-scripts/scss-ts-hack.js",
    "format": "prettier --write {src,dev-scripts}/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "lint:fix": "eslint --fix --ext js,ts,tsx src dev-scripts",
    "lint": "eslint --ext js,ts,tsx src dev-scripts",
    "postinstall": "yarn fix-dev-issues",
    "postuninstall": "yarn fix-dev-issues",
    "prepare": "husky install",
    "send-to-account": "cd hardhat && npx hardhat send-to-account --address",
    "start": "BROWSER=none react-scripts start",
    "test:watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false",
    "tsc:watch": "tsc --project tsconfig.json --watch",
    "tsc": "tsc --project tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn tsc"
    }
  },
  "lint-staged": {
    "{src,hardhat,dev-scripts}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/react": "^6.11.0",
    "@walletconnect/web3-provider": "^1.5.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "ethers": "^5.4.4",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.5.2",
    "react-helmet-async": "^1.0.9",
    "react-input-autosize": "^3.0.0",
    "react-number-format": "^4.7.3",
    "react-responsive-carousel": "^3.2.20",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@aragon/os": "^4.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@openzeppelin/contracts": "^4.2.0",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/chai": "^4.2.21",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-input-autosize": "^2.2.1",
    "@types/react-router-dom": "^5.1.8",
    "api3-dao": "git+https://github.com/api3dao/api3-dao.git#0b8bb17fbda0161d5d9fff00ed4562b91cc27af6",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "cypress": "^8.2.0",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "hardhat": "^2.6.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "node-sass": "^6.0.1",
    "npx": "^10.2.2",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "replace-in-file": "^6.2.0",
    "sort-package-json": "^1.50.0",
    "ts-node": "^10.2.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
